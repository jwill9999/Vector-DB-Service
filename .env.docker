# Local Docker overrides for docker-compose setup
HOST=0.0.0.0
PORT=8080
NODE_ENV=development

# Leave Supabase settings empty by default; supply real values to hit a remote Supabase project.
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_DIRECT_URL=postgresql://postgres:postgres@supabase:5432/postgres

SUPABASE_ANON_KEY=
SUPABASE_SCHEMA=public
SUPABASE_DOCUMENT_TABLE=documents
SUPABASE_CHUNK_TABLE=document_chunks
SUPABASE_MATCH_FUNCTION=match_document_chunks
SUPABASE_EMBEDDING_DIMENSIONS=1536

# Embeddings (stubbed locally unless you provide a real key)
EMBEDDING_PROVIDER=openai
OPENAI_API_KEY=
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Google configuration (leave blank for manual tests / curl)
GOOGLE_DRIVE_WATCH_FOLDER_ID=
GOOGLE_DRIVE_WEBHOOK_SECRET=local-dev-token
GOOGLE_SERVICE_ACCOUNT_EMAIL=
GOOGLE_SERVICE_ACCOUNT_KEY=
GOOGLE_PUBSUB_TOPIC=

INGESTION_QUEUE_NAME=google-docs-ingest
