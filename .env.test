# Integration test environment variables
# These mirror the local Docker defaults but target the Supabase container
# exposed on localhost:54322. Copy or override as needed for CI pipelines.
NODE_ENV=test

SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
SUPABASE_ANON_KEY=
SUPABASE_DIRECT_URL=postgresql://postgres:postgres@127.0.0.1:54322/postgres
SUPABASE_SCHEMA=public
SUPABASE_DOCUMENT_TABLE=documents
SUPABASE_CHUNK_TABLE=document_chunks
SUPABASE_MATCH_FUNCTION=match_document_chunks
SUPABASE_EMBEDDING_DIMENSIONS=1536

EMBEDDING_PROVIDER=openai
OPENAI_API_KEY=
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

INGESTION_QUEUE_NAME=google-docs-ingest
