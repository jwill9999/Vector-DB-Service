{
  "name": "vectordb",
  "version": "0.1.0",
  "description": "Basic scaffold for vector database tooling.",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install || true",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "commit": "npm run format && npm run lint:fix",
    "test": "npm run build && npm run test:run",
    "test:run": "node --test dist/routes/__tests__/*.test.js dist/services/supabase/__tests__/*.test.js",
    "test:coverage": "c8 --reporter=text --reporter=text-summary --reporter=lcov --reporter=html --reporter=json-summary npm run test:run"
  },
  "keywords": [
    "vector",
    "database",
    "typescript"
  ],
  "author": "Jason Williams",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.78.0",
    "googleapis": "^131.0.0",
    "openai": "^4.59.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-pretty": "^13.1.2"
  },
  "devDependencies": {
    "@types/node": "^20.14.9",
    "@types/pg": "^8.11.6",
    "@types/pino": "^7.0.4",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "c8": "^9.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "husky": "^9.1.5",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.5",
    "typescript": "^4.9.5"
  }
}
